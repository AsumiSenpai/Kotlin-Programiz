fun main(args: Array<String>) {

    first@ for (i in 1..2) {

        second@ for (j in 1..6) {
            println("i = $i; j = $j")

            if (i == 2)
                break@second
        }
    }
}

//hasil:
i = 1; j = 1
i = 1; j = 2
i = 1; j = 3
i = 1; j = 4
i = 1; j = 5
i = 1; j = 6
i = 2; j = 1

Process finished with exit code 0
